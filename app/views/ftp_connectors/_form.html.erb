<%= form_for @ftp_connector do |f| %>
	<%= f.label "name" %>
	<%= f.text_field "name" %>
	<%= f.label "username" %>
	<%= f.text_field "username" %>
	<%= f.label "password" %>
	<%= f.text_field "password" %>
	<%= f.label "host" %>
	<%= f.text_field "host" %>
	<%= f.label "port" %>
	<%= f.text_field "port" %>
	<%= f.submit "Save Connector" %>
<% end %>