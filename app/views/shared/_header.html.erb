<header>
	<span class="logo"></span>
	<nav>
		<div class="navbar-buttons">
			<% if current_user %>
				<%= link_to "Log out", logout_path, method: :delete %>
			<% else %>
				<span class="button navbar-button" id="signin-button"><%= link_to "Log in", login_path %></span>
				<span class="button navbar-button" id="signup-button"><%= link_to "Sign up", new_user_path %></span>
			<% end %>
		</div>
	</nav>
</header>